@article{buchanan-etal_measuring_journalarticle_2025,
	title = {Measuring the {Semantic} {Priming} {Effect} {Across} {Many} {Languages}},
	copyright = {https://creativecommons.org/licenses/by/4.0/legalcode},
  journal = {Nat Hum Behav},
	doi = {10.31219/osf.io/q4fjy},
	abstract = {Semantic priming has been studied for nearly 50 years across various experimental manipulations and theoretical frameworks. These studies provide insight into the cognitive underpinnings of semantic representations in both healthy and clinical populations; however, they have suffered from several issues including generally low sample sizes and a lack of diversity in linguistic implementations. Here, we will test the size and the variability of the semantic priming effect across ten languages by creating a large database of semantic priming values, based on an adaptive sampling procedure. Differences in response latencies between related word-pair conditions and unrelated word-pair conditions (i.e., difference score confidence interval is greater than zero) will allow quantifying evidence for semantic priming, whereas improvements in model fit with the addition of a random intercept for language will provide support for variability in semantic priming across languages.},
	author = {Buchanan, Erin Michelle and Cuccolo, Kelly and Heyman, Tom and Van Berkel, Niels and Coles, Nicholas Alvaro and Iyer, Aishwarya and Peters, Kim Olivia and Van 'T Veer, Anna Elisabeth and Montefinese, Maria and Maxwell, Nicholas P. and Taylor, Jack Edward and Valentine, Kathrene D and Arriaga, Patricia and Barzykowski, Krystian and Boucher, Leanne and Collins, W. Matthew and Vaidis, David C. and Aczel, Balazs and Al-Hoorie, Ali H. and Ambrosini, Ettore and Besson, Théo and Burin, Di and Butt, Muhammad Mussaffa and Clarke, Alan James Benjamin and Daryani, Yalda and El-Dakhs, Dina Abdel Salam and Elsherif, Mahmoud Medhat and Fernández-López, María and Ferreira, Paulo Rs and Freitag, Raquel Meister Ko. and Gattei, Carolina and Godbersen, Hendrik and Grim, Philip A. and Halama, Peter and Havan, Patrik and Irrazabal, Natalia and Isloi, Chris and Iversen, Rebecca and Julliard, Yoann and Karaaslan, Aslan and Kohút, Michal and Kohútová, Veronika and Kos, Julija and Kosachenko, Alexandra I. and Lima, Tiago Jessé Souza and Mak, Matthew Hc and Manouilidou, Christina and Marciaga, Leonardo and Xiaolin, Melinna and Miranda, Jacob Francisco and Morvinski, Coby and Muppoor, Aishwarya and Müjdeci, Fatma Elif and Nielsen, Yngwie Asbjørn and Oliveros, Juan Carlos and Onič, Jaš and Papadatou-Pastou, Marietta and Patel, Ishani and Pavlović, Zoran and Pažon, Blaž and Pfuhl, Gerit and Pronizius, Ekaterina and Roettger, Timo Benjamin and Ronderos, Camilo R. and Ruiz-Fernandez, Susana and Senderecka, Magdalena and Solak, Çağlar and Stückler, Anna and Szekely-Copîndean, Raluca Diana and Taboh, Analí and Thériault, Rémi and Tran, Ulrich S. and Trecca, Fabio and Ulloa Fulgeri, Jose Luis and Varga, Marton Aron and Verheyen, Steven and Pavlović, Tijana Vesić and Viviani, Giada and Wang, Nan and Živná, Kristýna and Yun, Chen Chu and Clark, Oliver James and Acar, Oguz Ali and Adamkovic, Matus and Agnoletti, Giulia and Akil, Atakan M. and Alsuhaibani, Zainab and Amenta, Simona and Ananye