@article{roettger-etal_Positional_journalarticle_2021,
  title = {Positional Biases in Predictive Processing of Intonation},
  author = {Roettger, Timo B. and Franke, Michael and Cole, Jennifer},
  year = {2021},
  month = apr,
  journal = {Language, Cognition and Neuroscience},
  volume = {36},
  number = {3},
  pages = {342--370},
  publisher = {Routledge},
  doi = {10.1080/23273798.2020.1853185},
  urldate = {2024-07-16},
  abstract = {Real-time speech comprehension is challenging because communicatively relevant information is distributed throughout the entire utterance. In five mouse tracking experiments on German and American English, we probe if listeners, in principle, use non-local, early intonational information to anticipate upcoming referents. Listeners had to select a speaker-intended referent with their mouse guided by intonational cues, allowing them to anticipate their decision by moving their hand toward the referent prior to lexical disambiguation. While German listeners (Exps. 1--3) seemed to ignore early pitch cues, American English listeners (Exps. 4--5) were in principle able to use these early pitch cues to anticipate upcoming referents. However, many listeners showed no indication of doing so. These results suggest that there are important positional asymmetries in the way intonational information is integrated, with early information being paid less attention to than later cues in the utterance. Open data, scripts, and materials can be retrieved here: https://osf.io/xf8be/.},
  keywords = {intonation,mouse tracking,Prosody,rational analysis,sentence comprehension},
  file = {/Users/timoroettger/Zotero/storage/5W3799WW/Roettger et al_2021_Positional biases in predictive processing of intonation.pdf}
}

